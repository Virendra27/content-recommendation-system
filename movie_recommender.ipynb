    "### 2.4 Data Cleaning, Preprocessing, and Feature Engineering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Merge datasets\n",
    "movies_df['id'] = movies_df['id'].astype(str)\n",
    "credits_df['movie_id'] = credits_df['movie_id'].astype(str)\n",
    "\n",
    "merged_df = movies_df.merge(credits_df, left_on='id', right_on='movie_id', how='inner')\n",
    "\n",
    "print(f\"âœ… Datasets merged successfully!\")\n",
    "print(f\"Merged dataset shape: {merged_df.shape}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Feature extraction functions\n",
    "def safe_json_parse(text):\n",
    "    \"\"\"Safely parse JSON strings\"\"\"\n",
    "    try:\n",
    "        if pd.isna(text) or text == '':\n",
    "            return []\n",
    "        return json.loads(text)\n",
    "    except:\n",
    "        return []\n",
    "\n",
    "def extract_genres(genres_json):\n",
    "    \"\"\"Extract genre names from JSON\"\"\"\n",
    "    genres = safe_json_parse(genres_json)\n",
    "    return [genre['name'] for genre in genres] if genres else []\n",
    "\n",
    "def extract_director(crew_json):\n",
    "    \"\"\"Extract director name from crew JSON\"\"\"\n",
    "    crew = safe_json_parse(crew_json)\n",
    "    for person in crew:\n",
    "        if person.get('job') == 'Director':\n",
    "            return person.get('name', 'Unknown')\n",
    "    return 'Unknown'\n",
    "\n",
    "def extract_cast(cast_json, n=3):\n",
    "    \"\"\"Extract top N cast members\"\"\"\n",
    "    cast = safe_json_parse(cast_json)\n",
    "    return [actor['name'] for actor in cast[:n]] if cast else []\n",
    "\n",
    "print(\"âœ… Feature extraction functions defined\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Apply feature extraction\n",
