    "---\n",
    "## 4. Core Implementation\n",
    "\n",
    "### 4.1 Recommendation Algorithm Implementation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_similar_movies(movie_name, movies_data, top_n=10):\n",
    "    \"\"\"\n",
    "    Find similar movies based on content features\n",
    "    \n",
    "    Parameters:\n",
    "    - movie_name: Name of the input movie\n",
    "    - movies_data: DataFrame with movie information\n",
    "    - top_n: Number of recommendations to return\n",
    "    \n",
    "    Returns:\n",
    "    - target_movie: Input movie details\n",
    "    - recommendations: List of (score, movie) tuples\n",
    "    \"\"\"\n",
    "    \n",
    "    # Find target movie\n",
    "    target_movie = None\n",
    "    for idx, row in movies_data.iterrows():\n",
    "        if row['original_title'].lower() == movie_name.lower():\n",
    "            target_movie = row\n",
    "            break\n",
    "    \n",
    "    if target_movie is None:\n",
    "        return None, []\n",
    "    \n",
    "    # Extract target movie features\n",
    "    target_genres = set(target_movie['genres_list'])\n",
    "    target_director = target_movie['director']\n",
    "    target_cast = set(target_movie['top_cast'])\n",
    "    \n",
    "    # Calculate similarity scores\n",
    "    similar_movies = []\n",
    "    \n",
    "    for idx, movie in movies_data.iterrows():\n",
    "        # Skip the target movie itself\n",
    "        if movie['id'] == target_movie['id']:\n",
    "            continue\n",
    "        \n",
    "        # Calculate feature matches\n",
    "        movie_genres = set(movie['genres_list'])\n",
    "        genre_matches = len(target_genres & movie_genres)\n",
    "        \n",
    "        director_match = 1 if movie['director'] == target_director else 0\n",
    "        \n",
    "        movie_cast = set(movie['top_cast'])\n",
    "        cast_matches = len(target_cast & movie_cast)\n",
    "        \n",
    "        # Calculate weighted similarity score\n",
    "        similarity_score = (\n",
    "            genre_matches * 2 +      # Genre weight: 2\n",
    "            director_match * 3 +     # Director weight: 3\n",
    "            cast_matches * 1         # Cast weight: 1\n",
    "        )\n",
    "        \n",
    "        # Only include movies with non-zero similarity\n",
    "        if similarity_score > 0:\n",
    "            similar_movies.append((similarity_score, movie))\n",
    "    \n",
    "    # Sort by similarity score (descending) and return top N\n",
    "    similar_movies.sort(reverse=True, key=lambda x: x[0])\n",
    "    \n",
    "    return target_movie, similar_movies[:top_n]\n",
    "\n",
    "print(\"âœ… Recommendation algorithm implemented successfully!\")"
   ]
  },
