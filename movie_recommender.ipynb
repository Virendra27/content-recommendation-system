    "def convert_gdrive_url(share_url):\n",
    "    \"\"\"Convert Google Drive sharing URL to direct download URL\"\"\"\n",
    "    try:\n",
    "        file_id = share_url.split('/d/')[1].split('/')[0]\n",
    "        return f'https://drive.google.com/uc?id={file_id}&export=download'\n",
    "    except Exception as e:\n",
    "        print(f\"Error converting URL: {e}\")\n",
    "        return share_url\n",
    "\n",
    "def read_csv_from_gdrive(url):\n",
    "    \"\"\"Read CSV file from Google Drive URL\"\"\"\n",
    "    try:\n",
    "        download_url = convert_gdrive_url(url)\n",
    "        print(f\"Downloading from Google Drive...\")\n",
    "        \n",
    "        response = requests.get(download_url, timeout=30)\n",
    "        response.raise_for_status()\n",
    "        \n",
    "        content = response.content.decode('utf-8')\n",
    "        df = pd.read_csv(io.StringIO(content))\n",
    "        \n",
    "        print(f\"âœ… Successfully loaded {len(df)} records\")\n",
    "        return df\n",
    "    except Exception as e:\n",
    "        print(f\"âŒ Error reading CSV from Google Drive: {e}\")\n",
    "        return None\n",
    "\n",
    "print(\"âœ… Google Drive helper functions defined\")"
