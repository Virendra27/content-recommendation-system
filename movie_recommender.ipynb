    "# Quantitative Analysis: Genre Match Rate\n",
    "def calculate_genre_match_rate(target_movie, recommendations):\n",
    "    \"\"\"Calculate how many recommended movies share at least one genre\"\"\"\n",
    "    target_genres = set(target_movie['genres_list'])\n",
    "    matches = 0\n",
    "    \n",
    "    for score, movie in recommendations:\n",
    "        movie_genres = set(movie['genres_list'])\n",
    "        if len(target_genres & movie_genres) > 0:\n",
    "            matches += 1\n",
    "    \n",
    "    return (matches / len(recommendations)) * 100 if recommendations else 0\n",
